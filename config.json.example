{
    "level": {
    	"compliance": "2"
    },
    "profile": {
    	"services": {
		    "enable": [ "palette" ]
	}
    },
    "palette": {
    	"extruder": { "name": "vibrant", "count": 5 },
    	"grid": { "name": "euclidian" },
	"palette": [
		    { "name": "crayola" },
		    { "name": "css4" }
        ]
    },
    "graphics": {
	"driver": "vip://"
    },
    "features": {
	"enable": {
	    "size": [ "max" ],
	    "format": [ "webp", "tif", "gif" ],
	    "rotation": [ "noAutoRotate" ]
	},
	"disable": {
	    "rotation": [ "rotationArbitrary" ],
	    "quality": [ "bitonal" ]
	},
	"append": {
	    "quality": {
		"dither": { "syntax": "dither", "required": false, "supported": true, "match": "^dither$" }
	    }
	}
    },
    "images": {
	"source": { "uri": "file://example/images" },
	"cache": { "uri": "memory://" }
    },
    "derivatives": {
	"cache": { "uri": "memory://"" }
    }
}
